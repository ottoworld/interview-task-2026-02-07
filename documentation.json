            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, signal } from '@angular/core';\r\nimport {TaskComponent} from \"../components/task/task.component\";\r\nimport {NgOptimizedImage} from \"@angular/common\";\r\n\r\nexport type TaskData = {\r\n  id: string;\r\n  media: string;\r\n  mediaAlt: string;\r\n  head: string;\r\n  body: string;\r\n  callToAction: string;\r\n  href?: string;\r\n  onClick?: () => void;\r\n}\r\n\r\n@Component({\r\n  selector: 'app-root',\r\n  imports: [TaskComponent, NgOptimizedImage],\r\n  templateUrl: './app.html',\r\n  styleUrl: './app.css'\r\n})\r\nexport class App {\r\n  onClick = () => { console.log('click'); };\r\n\r\n  tasks: TaskData[] = [\r\n    {\r\n      id: \"upload\",\r\n      body: \"Submit member and cashflow data to Hymans Robertson via our single secure data submission portal with real-time data validation.\",\r\n      head: \"Upload data\",\r\n      callToAction: \"Visit app\",\r\n      media: \"/Upload Data.png\",\r\n      mediaAlt: \"Upload data icon with pink background\",\r\n      onClick: this.onClick\r\n    }\r\n  ]\r\n  protected readonly title = signal('interview-task-2026-02-07');\r\n}\r\n",
            "styleUrl": "./app.css",
            "assetsDirs": [],
            "styleUrlsData": "",
            "stylesData": "",
            "extends": [],
            "templateData": "@for (task of tasks; track task.id) {\r\n  @if (task.href) {\r\n    <a task>{{task.body}}</a>\r\n  } @else {\r\n    <button task [callToAction]=\"task.callToAction\" [head]=\"task.head\">\r\n      {{task.body}}\r\n      <img slot=\"media\" [ngSrc]=\"task.media\" [alt]=\"task.mediaAlt\" height=\"144\" width=\"128\">\r\n    </button>\r\n  }\r\n}\r\n"
