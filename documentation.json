            "styleUrlsData": "",
            "stylesData": "",
            "extends": [],
            "templateData": "<div class=\"task-media\">\r\n  <img class=\"task-media__image\" [ngSrc]=\"media()!\" [alt]=\"mediaAlt()\" height=\"144\" width=\"128\" priority>\r\n</div>\r\n<div class=\"task-content\">\r\n  <h2 class=\"task-head\">{{head()}}</h2>\r\n  <p class=\"task-body\">\r\n    <ng-content></ng-content>\r\n  </p>\r\n  <p class=\"task-foot\">\r\n    {{callToAction()}}\r\n    <img class=\"task-foot__icon--light\" ngSrc=\"/icons/chevron-right-light.svg\" alt=\"chevron right\" height=\"18\" width=\"18\"/>\r\n    <img class=\"task-foot__icon--dark\" ngSrc=\"/icons/chevron-right-dark.svg\" alt=\"chevron right\" height=\"18\" width=\"18\"/>\r\n  </p>\r\n</div>\r\n"
